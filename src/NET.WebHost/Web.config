<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <connectionStrings>
    <add name="MyFood" connectionString="Server=.\SQLEXPRESS;Database=MyFood;Integrated Security=True;Asynchronous Processing=True;"/>
  </connectionStrings>
  <system.web>
    <compilation debug="true" targetFramework="4.8"/>
    <httpRuntime targetFramework="4.8"/>
  </system.web>
  <system.webServer>
    <handlers>
      <add name="IISHandler1" type="lennybacon.MyFood.IISHandler1, lennybacon.MyFood.WebHost" path="IISHandler" verb="GET,POST" />
    </handlers>
  </system.webServer>

</configuration>
